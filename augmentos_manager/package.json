    "react-test-renderer": "18.2.0",
    "reactotron-core-client": "^2.9.7",
    "reactotron-react-js": "^3.3.16",
    "reactotron-react-native": "^5.1.13",
    "reactotron-react-native-mmkv": "^0.2.8",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "expo": {}
}
